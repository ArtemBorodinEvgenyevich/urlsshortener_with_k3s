# Application
APP_ENV=development
LOG_LEVEL=info

# URLs Service PostgreSQL
URLS_POSTGRES_USER=urlshortener
URLS_POSTGRES_PASSWORD=urlshortener_password
URLS_POSTGRES_DB=urlshortener_db

# URLs Service Redis
URLS_REDIS_PASSWORD=urls_redis_password

# IAM Service PostgreSQL
IAM_POSTGRES_USER=iam_user
IAM_POSTGRES_PASSWORD=iam_password
IAM_POSTGRES_DB=iam_db

# IAM Service Redis
IAM_REDIS_PASSWORD=iam_redis_password

# Session settings
SESSION_TTL=720h
SESSION_ID_LENGTH=32
SESSION_CACHE_TTL=30s
